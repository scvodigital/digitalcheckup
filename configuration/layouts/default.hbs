<body class="mdc-theme--footer-dark-bg">
  <digitalcheckup-site>
    <div class="mdc-typography">
      {{#ifAny request.params.query.debug (contains request.url.hostname ".local") ~}}
        {{#if errors.[0]}}
          <div style="z-index:100;position:fixed;top:0;right:0;padding:5px;background-color:#ff0000;color:#ffffff;">
            Route Task Errors (F12)
          </div>
        {{/if}}
        {{#if logs.[0]}}
          <div style="z-index:100;position:fixed;bottom:0;right:0;padding:5px;background-color:#0000ff;color:#ffffff;"
               onclick="document.querySelector('#route-debug-log').style.display='block';">
            Route Log
          </div>
          <div style="z-index:100;position:fixed;bottom:0;left:0;right:0;padding:5px;background-color:#ededed;color:#000000;overflow:auto;height:50vh;display:none;border-top:1px solid #cccccc;" id="route-debug-log">
            <a style="position:fixed;right:25px;" onclick="document.querySelector('#route-debug-log').style.display='none';">x</a>
<pre style="white-space: pre-wrap;">
{{~#each logs}}
{{{this}}}

{{/each~}}
</pre>
          </div>
        {{/if}}
      {{/ifAny}}

      <header class="mdc-top-app-bar mdc-elevation--z6 top-bar-stuck" id="top-bar">
        {{!-- <div class="mdc-top-app-bar__row mdc-top-app-bar__lower scvo-tab-bar">
          <section class="mdc-top-app-bar__row mdc-top-app-bar__section--align-start">
            {{> components_scvo_bar}}
          </section>
        </div> --}}
        <div class="container-fluid">
          <div class="mdc-top-app-bar__row mdc-top-app-bar__upper">
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-start mdc-top-app-bar__section--logo">
              <a href="/">
                Digital Checkup
                {{#unless (compare @root.data.currentSite.name "default") ~}}
                <span class="mdc-typography--caption">
                  For {{@root.data.currentSite.for_plural}}
                </span>
                {{/unless ~}}
              </a>
            </section>
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end">
              <button class="mdc-top-app-bar__navigation-icon hide-lg" data-drawer-target="#mobile-menu" aria-label="Menu">
                <span class="far fa-fw fa-2x fa-bars"></span>
              </button>
            </section>
            <section class="mdc-top-app-bar__section mdc-top-app-bar__section--align-end hide-md hide-sm">
                {{#>components_tab_bar
                    extraClasses="mdc-tab-bar__auto-width"
                    id="main-tab-bar"}}
                  {{#each @root.context.metaData.menus.main ~}}
                    {{>components_tab
                        url=(stripTrailingSlash path)
                        labelClasses=""
                        active=(regexMatch @root.request.url.pathname route "gi")
                        label=label}}
                  {{/each ~}}
                {{/components_tab_bar}}
            </section>
          </div>
        </div>
      </header>

      <aside class="mdc-drawer mdc-drawer--modal" id="mobile-menu" data-mdc-auto-init="MDCDrawer">
        <div class="mdc-drawer__content">
          <nav class="mdc-list">
            {{#each @root.context.metaData.menus.main ~}}
            <a class="mdc-list-item {{#if (regexMatch @root.request.url.pathname route "gi")}} mdc-list-item--activated{{/if}}"
               href="{{stripTrailingSlash path}}" {{#if (regexMatch @root.request.url.pathname route "gi")}}aria-selected="true"{{else}}aria-hidden="true"{{/if}}>
              {{#if icon ~}}
              <span class="far fa-{{icon}} mdc-list-item__graphic" aria-hidden="true"></span>
              {{/if ~}}
              {{{label}}}
            </a>
            {{/each ~}}
          </nav>
        </div>
      </aside>

      <div class="mdc-drawer-scrim"></div>

      <main class="main-body mdc-toolbar-fixed-adjust{{#if layoutParts.sidebar}} drawer-container-permanent{{else}} drawer-container-temporary{{/if}}">
        {{#if layoutParts.sidebar ~}}
        <aside class="mdc-drawer mdc-drawer--occasional" id="sidebar-temporary" data-menu-button=".filter-search-button">
          {{{layoutParts.sidebar}}}
        </aside>
        <div class="mdc-drawer-scrim"></div>
        {{/if ~}}
        {{{layoutParts.body}}}
      </main>

      <footer class="mdc-layout__footer mdc-theme--footer-bg mdc-theme--on-footer">
        <div class="footer-main">
          <div class="container-fluid">
            <div class="mdc-layout-grid">
              <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone contact">
                  <h3>Contact</h3>
                  <ul class="fa-ul">
                    <li>
                      <span class="fa-li"><span class="fas fa-envelope"></span></span>
                      <a href="mailto:{{@root.data.currentSite.email}}">{{@root.data.currentSite.email}}</a>
                    </li>
                    <li>
                      <span class="fa-li"><span class="fas fa-phone fa-flip-horizontal"></span></span>
                      <a href="tel:{{replace @root.data.currentSite.phone ' ' ''}}">{{@root.data.currentSite.phone}}</a>
                    </li>
                    <!-- <li>
                      <span class="fa-li"><span class="fab fa-twitter"></span></span>
                      <a href="https://twitter.com/digitalcheckup" target="_blank">Twitter</a>
                    </li> -->
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet mdc-layout-grid__cell--span-4-phone help">
                  <h3>Information</h3>
                  <ul class="mdc-list">
                    <li class="mdc-list-item">
                      <a href="/about">About</a>
                    </li>
                    <li class="mdc-list-item">
                      <a href="/privacy">Privacy</a>
                    </li>
                    <li class="mdc-list-item">
                      <a href="/cookies">Cookies</a>
                    </li>
                    <li class="mdc-list-item">
                      <a href="/terms">Terms & conditions</a>
                    </li>
                  </ul>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-6-desktop mdc-layout-grid__cell--span-8-tablet mdc-layout-grid__cell--span-4-phone about">
                  <h3>About</h3>
                  <p>
                    {{@root.data.currentSite.title}} is run by the <a href="https://scvo.org">Scottish Council for Voluntary Organisations</a> in partnership with the <a href="{{@root.data.currentSite.partner.url}}">{{@root.data.currentSite.partner.title}}</a>. <a href="/about">Find out more</a>
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="footer-copyright">
          <div class="container-fluid">
            <div class="mdc-layout-grid">
              <div class="mdc-layout-grid__inner">
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-9-desktop mdc-layout-grid__cell--span-6-tablet mdc-layout-grid__cell--span-4-phone">
                  <p>
                    &copy; {{momentFormat (moment) "YYYY"}}. The Scottish Council for Voluntary Organisations (SCVO) is a Scottish Charitable Incorporated Organisation.<br />
                    Charity registered in Scotland <a href="https://www.oscr.org.uk/about-charities/search-the-register/charity-details?number=SC003558">SC003558</a>. Registered office Mansfield Traquair Centre, 15 Mansfield Place, Edinburgh EH3 6BB.
                  </p>
                </div>
                <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-2-tablet mdc-layout-grid__cell--span-4-phone text-right">
                  <a href="{{@root.data.currentSite.partner.url}}" class="brand-logo r-margin-full" title="{{@root.data.currentSite.partner.title}}">
                    <img src="{{~>asset_url url=(concat 'images/logos/' @root.data.currentSite.partner.logo_filename)~}}" alt="{{@root.data.currentSite.partner.title}}" />
                  </a>
                  <a href="https://scvo.org" class="brand-logo" title="Scottish Council for Voluntary Organisations">
                    {{!-- <img src="{{~>asset_url url='images/logos/scvo-legacy-black.png'~}}" alt="Scottish Council for Voluntary Organisations" /> --}}
                    <img src="{{~>asset_url url='images/logos/scvo-white.svg'~}}" alt="Scottish Council for Voluntary Organisations" />
                  </a>
                </div>
              </div>
            </div>
          </div>
        </div>
      </footer>
    </div>
  </digitalcheckup-site>

