{
  "domains": [
    "digitalcheckup.org",
    "digitalcheckup.local",
    "checkup.scot",
    "checkup.local",
    "digital.checkup.scot",
    "checkup-digital.local",
    "housing.digitalcheckup.org",
    "housing.checkup.scot",
    "checkup-housing.local",
    "cyber.checkup.scot",
    "checkup-cyber.local",
    "funding.checkup.scot",
    "checkup-funding.local",
    "governance.checkup.scot",
    "checkup-governance.local",
    "skills.checkup.scot",
    "checkup-skills.local"
  ],
  "routes": [
    "{:./routes/*.json}"
  ],
  "metaData": {
    "assetsVersion": "{:../package.json@version}",
    "assetsBase": "https://storage.googleapis.com/scvo-assets/test/digitalcheckup/",
    "title": "Digital Checkup",
    "domain-map": "{:./data/domain-map.json}",
    "site-configurations": {
      "root": "{:./data/sites/root.json}",
      "digital-legacy": "{:./data/sites/digital-legacy.json}",
      "digital": "{:./data/sites/digital.json}",
      "housing": "{:./data/sites/housing.json}",
      "cyber": "{:./data/sites/cyber.json}",
      "governance": "{:./data/sites/governance.json}",
      "skills": "{:./data/sites/skills.json}"
    },
    "areas": "{:./data/areas.json}",
    "elasticsearchUrl": "{$./data/elasticsearch-legacy-url.txt}",
    "handlebars": {
      "partials": {
        "{$./partials/**/*.*}": "includeRelativePath=false&regexExpression=(-)&regexOptions=gi&regexReplace=_"
      },
      "templates": {
        "{$./templates/**/*.*}": ""
      },
      "layouts": {
        "{$./layouts/**/*.*}": ""
      },
      "queries": {
        "{$./queries/**/*.*}": ""
      }
    },
    "commonTasks": "{:./data/common-tasks.json}"
  }
}